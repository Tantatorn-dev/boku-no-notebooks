ramanujan <- function(n){
n*log(n) - n + log(n*(1 + 4*n*(1+2*n)))/6 + log(pi)/2
}
combination <- function(n,k){
exp(ramanujan(n) - ramanujan(k) - ramanujan(n-k))
}
